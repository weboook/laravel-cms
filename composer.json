{
    "name": "webook/laravel-cms",
    "description": "A lightweight, file-based inline CMS for Laravel that allows direct editing of content on the page",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "cms",
        "inline-editing",
        "file-based",
        "content-management"
    ],
    "homepage": "https://github.com/weboook/laravel-cms",
    "support": {
        "issues": "https://github.com/weboook/laravel-cms/issues",
        "source": "https://github.com/weboook/laravel-cms"
    },
    "authors": [
        {
            "name": "Webook",
            "email": "hello@webook.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/view": "^10.0|^11.0",
        "illuminate/filesystem": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "symfony/dom-crawler": "^6.2|^7.0",
        "symfony/css-selector": "^6.2|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.5|^10.0|^11.0",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.4"
    },
    "autoload": {
        "psr-4": {
            "Webook\\LaravelCMS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webook\\LaravelCMS\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/pint --test"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webook\\LaravelCMS\\CMSServiceProvider"
            ],
            "aliases": {
                "CMS": "Webook\\LaravelCMS\\Facades\\CMS"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
