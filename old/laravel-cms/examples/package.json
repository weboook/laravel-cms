{
  "name": "laravel-cms-example",
  "version": "1.0.0",
  "description": "Example Laravel application with CMS integration",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "watch": "vite build --watch",
    "hot": "vite --host",
    "production": "vite build --mode production",
    "cms:build": "npm run build && php artisan cms:optimize",
    "cms:watch": "npm run watch & php artisan cms:watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint resources/js --ext .js,.vue",
    "lint:fix": "eslint resources/js --ext .js,.vue --fix",
    "format": "prettier --write 'resources/js/**/*.{js,vue,css,scss}'",
    "analyze": "npm run build -- --analyze"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "alpinejs": "^3.13.1",
    "axios": "^1.5.0",
    "cropperjs": "^1.6.1",
    "file-drop-element": "^0.2.6",
    "flatpickr": "^4.6.13",
    "lodash": "^4.17.21",
    "quill": "^1.3.7",
    "sortablejs": "^1.15.0",
    "trix": "^2.0.7",
    "tippy.js": "^6.3.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@types/alpinejs": "^3.7.2",
    "@types/cropperjs": "^1.3.1",
    "@types/lodash": "^4.14.199",
    "@types/sortablejs": "^1.15.3",
    "@vitejs/plugin-vue": "^4.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "sass": "^1.69.4",
    "tailwindcss": "^3.3.5",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.19"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "keywords": [
    "laravel",
    "cms",
    "content-management",
    "php",
    "javascript",
    "vue",
    "alpine",
    "tailwind"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/laravel-cms-example.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/laravel-cms-example/issues"
  },
  "homepage": "https://github.com/yourusername/laravel-cms-example#readme"
}